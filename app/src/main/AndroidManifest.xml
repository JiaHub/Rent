<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhh.rent">

    <!-- 读写权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/rent"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rent"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RentActivity" />
        <activity android:name=".activity.RecordActivity" />
        <activity android:name=".activity.CalculateActivity" />
        <activity android:name=".record.RecordContentActivity"></activity>
    </application>

</manifest>